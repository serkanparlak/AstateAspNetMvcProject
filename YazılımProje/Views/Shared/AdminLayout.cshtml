@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title</title>
    <link rel="icon" type="image/png" sizes="16x16" href="~/ContentAdmin/images/favicon.png">
    <!-- Bootstrap Core CSS -->
    <link href="~/Content/Bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Menu CSS -->
    <link href="~/ContentAdmin/bower_components/sidebar-nav/dist/sidebar-nav.min.css" rel="stylesheet" />
    <!-- animation CSS -->
    <link href="~/ContentAdmin/css/animate.css" rel="stylesheet" />
    <!-- Custom CSS -->
    <link href="~/ContentAdmin/css/style.css" rel="stylesheet" />
    <!-- color CSS -->
    <link href="~/ContentAdmin/css/colors/default.css" rel="stylesheet" />

</head>

<body class="fix-header">
    <!-- ============================================================== -->
    <!-- Preloader -->
    <!-- ============================================================== -->
    <div class="preloader">
        <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
        </svg>
    </div>
    <!-- ============================================================== -->
    <!-- Wrapper -->
    <!-- ============================================================== -->
    <div id="wrapper">
        <!-- ============================================================== -->
        <!-- Topbar header - style you can find in pages.scss -->
        <!-- ============================================================== -->
        <nav class="navbar navbar-default navbar-static-top m-b-0">
            <div class="navbar-header">
                <div class="top-left-part">
                    <!-- Logo -->
                    <a class="logo" href="/Admin/Index/">
                        <!-- Logo icon image, you can use font-icon also --><b>
                            <!--This is dark logo icon--><img src="~/ContentAdmin/images/admin-logo.png" alt="home" class="dark-logo" /><!--This is light logo icon--><img src="~/ContentAdmin/images/admin-logo-dark.png" alt="home" class="light-logo" />
                        </b>
                        <!-- Logo text image you can use text also --><span class="hidden-xs">
                            <!--This is dark logo text--><img src="~/ContentAdmin/images/admin-text.png" alt="home" class="dark-logo" /><!--This is light logo text--><img src="~/ContentAdmin/images/admin-text-dark.png" alt="home" class="light-logo" />
                        </span>
                    </a>
                </div>
                <!-- /Logo -->
                <ul class="nav navbar-top-links navbar-right pull-right">
                    <li>
                        <form role="search" class="app-search hidden-sm hidden-xs m-r-10">
                            <input type="text" placeholder="Search..." class="form-control"> <a href=""><i class="fa fa-search"></i></a>
                        </form>
                    </li>




                    <li class="dropdown user user-menu">
                        <a href="#" class="profile-pic" data-toggle="dropdown" aria-expanded="true">
                            <img src="~/ContentAdmin/images/users/serkan.jpg" class="img-circle" alt="User Image" style="width:36px">
                            <b class=" hidden-xs">@Session["kAdi"]</b>
                        </a>
                        <ul class="dropdown-menu" style="padding-bottom: 0;">
                            <!-- User image -->
                            <li class=" user-header">
                            <img src="/ContentAdmin/images/users/serkan.jpg" class="img-circle" alt="User Image" style="padding: 20px; border-radius:0">

                            <p class="text-center">
                                @Session["kAdi"]
                                <br />
                                <small>Web Developer</small>
                            </p>
</li>
                            <!-- Menu Footer-->
                            <li class="user-footer">
                                <a href="/Home/CikisYap/" class="btn btn-danger btn-sm" style="border-radius: 0;">Oturumu Kapat</a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-header -->
            <!-- /.navbar-top-links -->
            <!-- /.navbar-static-side -->
        </nav>
        <!-- End Top Navigation -->
        <!-- ============================================================== -->
        <!-- Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <div class="navbar-default sidebar" role="navigation">
            <div class="sidebar-nav slimscrollsidebar">
                <ul class="nav" id="side-menu">
                    <li style="padding: 70px 0 0;">
                        <a href="/Admin/Users/" class="waves-effect"><i class="fa fa-user fa-fw" aria-hidden="true"></i>Kullanıcılar</a>
                    </li>
                    <li>
                        <a href="/Admin/IlanTurleri/" class="waves-effect"><i class="fa fa-bookmark-o fa-fw" aria-hidden="true"></i>Ilan Türleri</a>
                    </li>
                    <li>
                        <a href="/Admin/Mesajlar/" class="waves-effect"><i class="fa fa-comments-o fa-fw" aria-hidden="true"></i>Mesajlar</a>
                    </li>
                    <li>
                        <a href="/Admin/KonutListele/" class="waves-effect"><i class="fa  fa-xing fa-fw" aria-hidden="true"></i>Konutlar</a>
                    </li>
                    <li>
                        <a href="/Admin/IsyeriListele/" class="waves-effect"><i class="fa  fa-xing fa-fw" aria-hidden="true"></i>İşyerleri</a>
                    </li>
                    <li>
                        <a href="/Admin/ArsaListele/" class="waves-effect"><i class="fa  fa-xing fa-fw" aria-hidden="true"></i>Arsalar</a>
                    </li>
                </ul>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- End Left Sidebar -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Page Content -->
        <!-- ============================================================== -->
        <div id="page-wrapper">
            <div class="container-fluid">
                @RenderBody()
            </div>
            <!-- /.container-fluid -->
            <footer class="footer text-center"> 2017 &copy; UltraEmlak Admin</footer>
        </div>
        <!-- ============================================================== -->
        <!-- End Page Content -->
        <!-- ============================================================== -->
    </div>
    <!-- /#wrapper -->
    <!-- jQuery -->
    <script src="~/ContentAdmin/bower_components/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="~/ContentAdmin/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- Menu Plugin JavaScript -->
    <script src="~/ContentAdmin/bower_components/sidebar-nav/dist/sidebar-nav.min.js"></script>
    <!--slimscroll JavaScript -->
    <script src="~/ContentAdmin/js/jquery.slimscroll.js"></script>
    <!--Wave Effects -->
    <script src="~/ContentAdmin/js/waves.js"></script>
    <!-- Custom Theme JavaScript -->
    <script src="~/ContentAdmin/js/custom.min.js"></script>
</body>

</html>

